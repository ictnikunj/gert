<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="BstCronManager6\Command\CronCommand">
            <argument type="service" id="Shopware\Core\Framework\MessageQueue\ScheduledTask\Command\ScheduledTaskRunner"/>
            <argument type="service" id="console.command.messenger_consume_messages"/>
            <tag name="console.command"/>
        </service>
    </services>
</container>