<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
           https://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>

        <service id="PimImport\Core\Content\Pim\PimDefinition">
            <tag name="shopware.entity.definition" entity="pim_product" />
        </service>

        <service id="PimImport\Core\Content\PimCategory\PimCategoryDefinition">
            <tag name="shopware.entity.definition" entity="pim_category" />
        </service>

        <service id="PimImport\Controller\ProductImportController" public="true">
            <argument type="service" id="Shopware\Core\System\SystemConfig\SystemConfigService" />
            <argument type="service" id="product.repository"/>
            <argument type="service" id="language.repository"/>
            <argument type="service" id="product_manufacturer.repository"/>
            <argument type="service" id="tax.repository"/>
            <argument type="service" id="pim_product.repository"/>
            <argument type="service" id="product_cross_selling.repository"/>
            <argument type="service" id="product_cross_selling_assigned_products.repository"/>
            <argument type="service" id="media.repository"/>
            <argument type="service" id="product_media.repository"/>
            <argument type="service" id="Shopware\Core\Content\Media\MediaService" />
            <argument type="service" id="Shopware\Core\Content\Media\File\FileSaver" />
            <argument type="service" id="sales_channel.repository" />
            <argument type="service" id="product_visibility.repository" />
            <argument type="service" id="property_group_option.repository" />
            <argument type="service" id="acris_product_download.repository" />
            <argument type="service" id="ict_media_redirect.repository" />
            <argument type="service" id="media_folder.repository" />
            <argument type="service" id="snippet.repository" />
            <argument type="service" id="snippet_set.repository" />
            <argument type="service" id="product_property.repository" />
            <argument type="service" id="media_thumbnail_size.repository" />
            <argument type="service" id="property_group.repository"/>
            <argument type="service" id="pim_category.repository" />
            <call method="setContainer">
                <argument type="service" id="service_container"/>
            </call>
        </service>



        <service id="PimImport\Controller\ProductCrossImportController" public="true">
            <argument type="service" id="Shopware\Core\System\SystemConfig\SystemConfigService" />
            <argument type="service" id="product.repository"/>
            <argument type="service" id="language.repository"/>
            <argument type="service" id="pim_product.repository"/>
            <argument type="service" id="product_cross_selling.repository"/>
            <argument type="service" id="product_cross_selling_assigned_products.repository"/>
            <call method="setContainer">
                <argument type="service" id="service_container"/>
            </call>
        </service>

        <service id="PimImport\Controller\ProductCrossProductPartImportController" public="true">
            <argument type="service" id="Shopware\Core\System\SystemConfig\SystemConfigService" />
            <argument type="service" id="product.repository"/>
            <argument type="service" id="language.repository"/>
            <argument type="service" id="pim_product.repository"/>
            <argument type="service" id="product_cross_selling.repository"/>
            <argument type="service" id="product_cross_selling_assigned_products.repository"/>
            <call method="setContainer">
                <argument type="service" id="service_container"/>
            </call>
        </service>

        <service id="PimImport\Controller\ProductCrossAddonImportController" public="true">
            <argument type="service" id="Shopware\Core\System\SystemConfig\SystemConfigService" />
            <argument type="service" id="product.repository"/>
            <argument type="service" id="language.repository"/>
            <argument type="service" id="pim_product.repository"/>
            <argument type="service" id="product_cross_selling.repository"/>
            <argument type="service" id="product_cross_selling_assigned_products.repository"/>
            <call method="setContainer">
                <argument type="service" id="service_container"/>
            </call>
        </service>

        <service id="PimImport\Controller\CategoryImportController" public="true">
            <argument type="service" id="Shopware\Core\System\SystemConfig\SystemConfigService" />
            <argument type="service" id="category.repository"/>
            <argument type="service" id="language.repository"/>
            <argument type="service" id="media.repository"/>
            <argument type="service" id="Shopware\Core\Content\Media\MediaService" />
            <argument type="service" id="Shopware\Core\Content\Media\File\FileSaver" />
            <argument type="service" id="cms_page.repository" />
            <argument type="service" id="product.repository"/>
            <argument type="service" id="product_category.repository"/>
            <argument type="service" id="sales_channel.repository" />
            <argument type="service" id="pim_category.repository" />
            <argument type="service" id="kplngi_productcategoryposition.repository" />
            <argument type="service" id="kplngi_orderactive.repository" />
            <call method="setContainer">
                <argument type="service" id="service_container"/>
            </call>
        </service>

        <service id="PimImport\Controller\CategoryManageOrderController" public="true">
            <argument type="service" id="Shopware\Core\System\SystemConfig\SystemConfigService" />
            <argument type="service" id="category.repository"/>
            <argument type="service" id="language.repository"/>
            <argument type="service" id="media.repository"/>
            <argument type="service" id="Shopware\Core\Content\Media\MediaService" />
            <argument type="service" id="Shopware\Core\Content\Media\File\FileSaver" />
            <argument type="service" id="cms_page.repository" />
            <argument type="service" id="product.repository"/>
            <argument type="service" id="sales_channel.repository" />
            <argument type="service" id="kplngi_productcategoryposition.repository" />
            <argument type="service" id="kplngi_orderactive.repository" />
            <argument type="service" id="pim_category.repository" />
            <call method="setContainer">
                <argument type="service" id="service_container"/>
            </call>
        </service>

        <service id="PimImport\Controller\DeleteCategoryController" public="true">
            <argument type="service" id="Shopware\Core\System\SystemConfig\SystemConfigService" />
            <argument type="service" id="sales_channel.repository" />
            <argument type="service" id="category.repository"/>
            <argument type="service" id="language.repository"/>
            <argument type="service" id="pim_category.repository"/>
            <call method="setContainer">
                <argument type="service" id="service_container"/>
            </call>
        </service>

        <service id="PimImport\Controller\PropertyImportController" public="true">
            <argument type="service" id="Shopware\Core\System\SystemConfig\SystemConfigService" />
            <argument type="service" id="language.repository"/>
            <argument type="service" id="property_group.repository"/>
            <call method="setContainer">
                <argument type="service" id="service_container"/>
            </call>
        </service>

        <service id="PimImport\Controller\ProductPropertyImportController" public="true">
            <argument type="service" id="Shopware\Core\System\SystemConfig\SystemConfigService" />
            <argument type="service" id="language.repository"/>
            <argument type="service" id="property_group.repository"/>
            <argument type="service" id="product.repository"/>
            <argument type="service" id="property_group_option.repository"/>
            <argument type="service" id="product_property.repository" />
            <call method="setContainer">
                <argument type="service" id="service_container"/>
            </call>
        </service>

        <service id="PimImport\Controller\CategoryCronImportController" public="true">
            <argument type="service" id="Shopware\Core\System\SystemConfig\SystemConfigService" />
            <argument type="service" id="category.repository"/>
            <argument type="service" id="language.repository"/>
            <argument type="service" id="media.repository"/>
            <argument type="service" id="Shopware\Core\Content\Media\MediaService" />
            <argument type="service" id="Shopware\Core\Content\Media\File\FileSaver" />
            <argument type="service" id="cms_page.repository" />
            <argument type="service" id="product.repository"/>
            <argument type="service" id="sales_channel.repository" />
            <argument type="service" id="pim_category.repository" />
            <argument type="service" id="kplngi_productcategoryposition.repository" />
            <argument type="service" id="kplngi_orderactive.repository" />
            <argument type="service" id="category_cron_saleschannel.repository" />
            <call method="setContainer">
                <argument type="service" id="service_container"/>
            </call>
        </service>

        <service id="PimImport\Controller\ManuallyRunCronController" public="true">
            <argument type="service" id="Shopware\Core\System\SystemConfig\SystemConfigService" />
            <argument type="service" id="scheduled_task.repository" />
            <argument type="service" id="category_cron_saleschannel.repository" />
            <call method="setContainer">
                <argument type="service" id="service_container"/>
            </call>
        </service>
    </services>
</container>
